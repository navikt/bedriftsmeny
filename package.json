{
    "name": "bedriftsmeny",
    "version": "1.0.0",
    "description": "Bedriftsvelger og -meny for innlogget arbeidsgiver. Laget av TAG (Tjenester for Arbeidsgivere).",
    "author": "Team TAG",
    "license": "MIT",
    "main": "index.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/bedriftsmeny.git"
    },
    "scripts": {
        "start:parcel": "parcel src/index.html --open --port 8000",
        "start:ts": "node_modules/typescript/bin/tsc --watch --noEmit",
        "start": "run-p start:parcel start:ts",
        "build": "parcel build src/index.html -d build",
        "serve": "serve -s build"
    },
    "dependencies": {
        "react": "^16.9.0",
        "react-dom": "^16.9.0"
    },
    "devDependencies": {
        "@types/react": "^16.9.1",
        "@types/react-dom": "^16.8.5",
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.3",
        "serve": "^11.1.0",
        "typescript": "^3.5.3"
    }
}
