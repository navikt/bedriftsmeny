{
    "name": "@navikt/bedriftsmeny",
    "version": "5.0.0",
    "description": "Bedriftsvelger og -meny for innlogget arbeidsgiver. Laget av TAG (Tjenester for Arbeidsgivere).",
    "author": "NAVIKT",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/bedriftsmeny.git"
    },
    "source": "src/index.html",
    "main": "lib/Bedriftsmeny.js",
    "types": "lib/Bedriftsmeny.d.ts",
    "files": [
        "lib",
        "src/bedriftsmeny"
    ],
    "scripts": {
        "start:parcel": "parcel",
        "start:ts": "tsc --watch",
        "start": "run-p start:parcel start:ts",
        "clean": "rm -rf lib",
        "prebuild": "npm run clean",
        "build": "run-p build:ts build:css",
        "build:ts": "tsc --project tsconfig.build.json",
        "build:css": "postcss --use postcss-import -o lib/bedriftsmeny.css src/bedriftsmeny/index.css"
    },
    "peerDependencies": {
        "@types/react": "17 || 18",
        "react": "17 || 18",
        "@types/react-dom": "17 || 18",
        "react-dom": "17 || 18",
        "react-router-dom": "6"
    },
    "peerDependenciesMeta": {
        "react-router-dom": {
            "optional": true
        }
    },
    "devDependencies": {
        "@parcel/transformer-less": "^2.8.0",
        "@types/history": "^4.7.8",
        "@types/react": "^18.0.25",
        "@types/react-collapse": "^5.0.0",
        "@types/react-dom": "^18.0.8",
        "@types/react-modal": "^3.13.1",
        "npm-run-all": "^4.1.5",
        "parcel": "^2.8.0",
        "postcss": "^8.4.19",
        "postcss-cli": "^10.0.0",
        "postcss-import": "^15.0.0",
        "prettier": "2.2.1",
        "process": "^0.11.10",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@navikt/ds-css": "^1.5.6",
        "@navikt/ds-icons": "^1.5.6",
        "@navikt/ds-react": "^1.5.6",
        "@types/amplitude-js": "^8.9.4",
        "fuzzysort": "^1.1.4",
        "history": "^4.10.1"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 100,
        "singleQuote": true
    }
}
