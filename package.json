{
    "name": "@navikt/bedriftsmeny",
    "version": "8.0.0",
    "description": "Bedriftsvelger og -meny for innlogget arbeidsgiver. Laget av TAG (Tjenester for Arbeidsgivere).",
    "author": "NAVIKT",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/bedriftsmeny.git"
    },
    "source": "src/index.html",
    "main": "lib/cjs/Bedriftsmeny.js",
    "module": "lib/esm/Bedriftsmeny.js",
    "types": "lib/types/Bedriftsmeny.d.ts",
    "files": [
        "lib",
        "src/bedriftsmeny"
    ],
    "scripts": {
        "clean": "rm -rf lib",
        "prebuild": "npm run clean",
        "build": "run-p build:ts build:css",
        "build:ts": "tsc -b ./tsconfig.build.cjs.json ./tsconfig.build.esm.json ./tsconfig.build.types.json",
        "build:css": "postcss --use postcss-import -o lib/bedriftsmeny.css src/bedriftsmeny/index.css",
        "dev": "next dev"
    },
    "peerDependencies": {
        "@navikt/ds-css": ">=7.21",
        "@navikt/ds-react": ">=7.21",
        "@types/react": "18 || 19",
        "@types/react-dom": "18 || 19",
        "react": "18 || 19",
        "react-dom": "18 || 19",
        "react-router": "^7.5.3"
    },
    "devDependencies": {
        "@navikt/ds-css": "^7.21.0",
        "@navikt/ds-react": "^7.21.0",
        "@types/history": "^5.0.0",
        "@types/node": "22.15.3",
        "@types/react": "^19.1.2",
        "@types/react-collapse": "^5.0.4",
        "@types/react-dom": "^19.1.3",
        "@types/react-modal": "^3.16.3",
        "next": "15.3.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.5.3",
        "postcss-cli": "^11.0.1",
        "postcss-import": "^16.1.0",
        "prettier": "3.5.3",
        "process": "^0.11.10",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "focus-trap-react": "^11.0.3",
        "fuzzysort": "^3.1.0",
        "history": "^5.3.0"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 100,
        "singleQuote": true
    }
